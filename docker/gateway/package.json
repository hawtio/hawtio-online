{
  "name": "@hawtio/gateway",
  "version": "2.1.0",
  "description": "The hawtio-online gateway to jolokia on kubernetes",
  "author": "Hawtio developer team",
  "license": "Apache-2.0",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/hawtio/hawtio-online.git"
  },
  "keywords": [
    "hawtio",
    "jolokia"
  ],
  "scripts": {
    "start:gateway": "yarn start:gateway-dev & yarn start:gateway-server",
    "start:gateway-dev": "node --env-file .env.development proxy-dev-server.js",
    "start:gateway-server": "tsup --config tsup.config.dev.ts --watch",
    "build": "webpack --mode production --progress --config webpack.config.prod.js",
    "test": "jest --detectOpenHandles",
    "test:watch": "jest --detectOpenHandles --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "fetch-intercept": "^2.4.0",
    "helmet": "^8.1.0",
    "http-proxy-middleware": "^3.0.5",
    "https": "^1.0.0",
    "jolokia.js": "^2.3.0",
    "jwt-decode": "^4.0.0",
    "method-override": "^3.0.0",
    "pino": "^9.11.0",
    "pino-http": "^10.5.0",
    "pino-pretty": "^13.0.0",
    "yaml": "^2.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/method-override": "^3.0.0",
    "@types/node": "^24.5.2",
    "@types/pino-multi-stream": "^5.1.6",
    "@types/supertest": "^6.0.2",
    "babel-jest": "^29.6.1",
    "compression-webpack-plugin": "^11.1.0",
    "copy-webpack-plugin": "^13.0.0",
    "jest": "^30.1.3",
    "jest-extended": "^6.0.0",
    "jest-fetch-mock": "^3.0.3",
    "source-map-loader": "^5.0.0",
    "supertest": "^7.1.3",
    "ts-jest": "^29.4.4",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths-webpack-plugin": "^4.2.0",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "webpack": "^5.101.3",
    "webpack-cli": "^6.0.1"
  },
  "packageManager": "yarn@4.3.1",
  "engines": {
    "yarn": ">=4.3.1",
    "node": ">=20"
  },
  "publishConfig": {
    "access": "private"
  }
}
