  "version": "0.0.0",
  "description": "An API for interacting with kubernetes clusters",
  "main": "dist/hawtio-kubernetes-api.js",
  "types": "dist/hawtio-kubernetes-api.d.ts",
  "author": "Hawtio developer team",
  "license": "Apache-2.0",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/hawtio/hawtio-online.git"
  },
  "keywords": [
    "hawtio",
    "camel",
    "console",
    "jmx",
    "react"
  ],
  "scripts": {
