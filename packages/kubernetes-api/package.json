    "jquery": "^3.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^8.0.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "urijs": "^1.19.11"
  },
  "devDependencies": {
    "@types/jest": "^29.5.2",
    "@types/jquery": "^3.5.16",
    "@types/urijs": "^1.19.19",
    "eslint-config-react-app": "^7.0.1",
    "hawtio-node-backend": "^2.0.5",
    "jest": "^29.6.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-fetch-mock": "^3.0.3",
    "jest-watch-typeahead": "^2.2.2",
    "js-logger": "^1.6.1",
    "replace": "^1.2.2",
    "ts-jest": "^29.1.0",
    "tsup": "^7.0.0"
